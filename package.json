{
  "name": "gitranks-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "@biomejs/biome check",
    "format": "@biomejs/biome format --write",
    "types": "next typegen && tsc --noEmit",
    "codegen": "dotenv -e .env.local graphql-codegen",
    "standalone": "dotenv -e .env.local node .next/standalone/server.js",
    "icons": "pnpm dlx @svgr/cli --out-dir components/icons --filename-case kebab --typescript --no-dimensions --replace-attr-values \"#000=currentColor\" -- public/icons",
    "figma:icons": "tsc lib/figma/download-icons.ts --outDir dist && dotenv -e .env.local node dist/download-icons.js && pnpm run icons",
    "indexnow": "dotenv -e .env.local -- tsx lib/indexnow/indexnow-submit.ts"
  },
  "browserslist": {
    "production": [
      "supports es6-module",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@auth/mongodb-adapter": "^3.11.0",
    "@hookform/resolvers": "^5.2.2",
    "@next/third-parties": "^15.5.6",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@uiw/color-convert": "^2.9.0",
    "@uiw/react-color-colorful": "^2.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "country-flag-emoji-polyfill": "^0.1.8",
    "date-fns": "^4.1.0",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-fade": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.527.0",
    "mongodb": "^6.20.0",
    "next": "16.0.0-canary.16",
    "next-auth": "5.0.0-beta.28",
    "next-themes": "^0.4.6",
    "nuqs": "^2.7.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.65.0",
    "react-icons": "^5.5.0",
    "react-spinners": "^0.17.0",
    "recharts": "^3.3.0",
    "satori": "^0.16.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "usehooks-ts": "^3.1.1",
    "vaul": "^1.1.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.6",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/typed-document-node": "^5.1.2",
